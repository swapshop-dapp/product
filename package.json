{
  "name": "nestjs-template",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run prisma generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "migrate": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --maxWorkers=1 --passWithNoTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:cron-update-contact": "node dist/src/cronjobs/update-contact.cron",
    "start:cron-sync-property-price": "node dist/src/cronjobs/sync-property-price.cron",
    "prisma": "./node_modules/.bin/prisma"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.549.0",
    "@mailchimp/mailchimp_transactional": "^1.0.59",
    "@nestjs/axios": "^0.0.6",
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^1.2.1",
    "@nestjs/core": "^9.4.3",
    "@nestjs/event-emitter": "^1.4.2",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/schedule": "^2.2.3",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/terminus": "^8.1.1",
    "@prisma/client": "^4.16.2",
    "@slack/web-api": "^6.12.0",
    "aws-sdk": "^2.1593.0",
    "axios": "^0.27.2",
    "bluebird": "^3.7.2",
    "cache-manager": "^3.6.3",
    "cache-manager-redis-store": "^2.0.0",
    "camelcase-keys": "^7.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "dayjs": "^1.11.10",
    "express": "^4.19.2",
    "helmet": "^5.1.1",
    "ical-generator": "^4.1.0",
    "image-url-validator": "^1.0.4",
    "ioredis": "^5.3.2",
    "is-url": "^1.2.4",
    "kafkajs": "^1.16.0",
    "lodash": "^4.17.21",
    "luxon": "^2.5.2",
    "netmask": "^2.0.2",
    "node-ical": "^0.16.1",
    "passport": "^0.6.0",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.1",
    "psl": "^1.9.0",
    "qs": "^6.12.0",
    "reflect-metadata": "^0.1.14",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.31.3",
    "snakecase-keys": "^5.5.0",
    "swagger-ui-express": "^4.6.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@jest/transform": "^28.1.3",
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.3",
    "@side/jest-runtime": "^1.1.0",
    "@types/cache-manager": "^4.0.6",
    "@types/express": "^4.17.21",
    "@types/is-url": "^1.2.32",
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.17.0",
    "@types/luxon": "^2.4.0",
    "@types/multer": "^1.4.11",
    "@types/netmask": "^2.0.5",
    "@types/node": "^16.18.94",
    "@types/passport-jwt": "^3.0.13",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^26.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^7.0.4",
    "jest": "^28.1.3",
    "jest-runtime": "^28.1.3",
    "lint-staged": "^12.5.0",
    "prettier": "^3.2.5",
    "prisma": "^4.16.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^3.15.0",
    "typescript": "^4.9.5",
    "webpack": "^5.91.0"
  },
  "jest": {
    "runtime": "@side/jest-runtime",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.(e2e-)?spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s",
      "!src/main.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "lines": 0
      }
    },
    "coveragePathIgnorePatterns": [
      "src/common/",
      "dist/",
      "node_modules/"
    ],
    "testPathIgnorePatterns": [
      "src/common/",
      "dist/",
      "node_modules/"
    ]
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix"
  }
}
